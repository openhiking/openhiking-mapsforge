<?xml version="1.0"?>
<translations>
	<translation>
		<name>Propagate access restriction to foot</name>
		<description>Set foot=no if access is restricted</description>
        <match type="way" >
            <match type="way" mode="or">
                <tag k="access" v="no"/>
                <tag k="access" v="private"/>                
                <tag k="access" v="agricultural"/>				
                <tag k="access" v="forestry"/>				
            </match>
            <notag k="foot" v=".*"/>
        </match>            
		<output>
			<copy-all/>
			<tag k="foot" v="no"/>
		</output>
	</translation>
